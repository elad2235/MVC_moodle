@model Mvcproject.ModelView.GradesViewModel
@using Mvcproject.Models;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>
    <div>

        <div>
            <span style="color:blue">Grades </span>
            <br />
            <table>
                <tr>
                    
                    <td>student id</td>
                    <td>moed A grade</td>
                    <td>moed B grade</td>

                </tr>
                @foreach (Grades obj in Model.grades)
                {
                    <tr>
                       
                        <td>@obj.student_id</td>
                        <td>@obj.moedA</td>
                        <td>@obj.moedB</td>
                        <td>@Html.ActionLink("Edit", "edit_grade", obj, new { @class = "classname" })</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</body>
</html>
